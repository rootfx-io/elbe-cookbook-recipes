    <pkg>systemd-resolved</pkg>
    <pkg>systemd-timesyncd</pkg>
    <pkg>vim-tiny</pkg>
    <pkg>busybox</pkg>
    <pkg>bash</pkg>
    <pkg>openssh-server</pkg>
    <pkg>unzip</pkg>
    <pkg>i2c-tools</pkg>
    <pkg>can-utils</pkg>
    <pkg>usbutils</pkg>
    <pkg>pciutils</pkg>
    <pkg>mmc-utils</pkg>
    <pkg>iptables</pkg>
    <pkg>bridge-utils</pkg>
</pkg-list>
</target>
