</package>
<debootstrap>
    <variant>minbase</variant>
</debootstrap>

<pkg-list>
    <pkg>linux-image-arm64</pkg>
    <pkg>firmware-brcm80211</pkg>
    <pkg>raspi-firmware</pkg>
    <pkg>zstd</pkg>
