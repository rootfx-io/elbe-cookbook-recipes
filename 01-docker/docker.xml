    </package>
    <finetuning>
      <mkdir>/workspace</mkdir>
      <adduser passwd_hashed="$6$bOQ4Sf4rSdI1IfRd$3Hai04jz2OYEW108SZWeweGO/rGvO65ITthDDtDuaC/8Gpwek/xT4rUvKBkBHFIjTDd6.klFdNJ685kxgabHh/" shell="/bin/bash" uid="1000" create_group="true">rfx</adduser>
