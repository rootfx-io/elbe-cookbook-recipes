      <pkg>flex</pkg>
      <pkg>libssl-dev</pkg>
      <pkg>libc6-dev</pkg>
      <pkg>libncurses5-dev</pkg>
      <pkg>crossbuild-essential-arm64</pkg>
      <pkg>gcc</pkg>
    </pkg-list>
  </target>
